{
  "name": "first-follow-gh-pages",
  "version": "1.0.0",
  "private": "true",
  "scripts": {
    "lint:js": "eslint --ext '.js,jsx' ./",
    "lint:styles": "stylelint ./src/**/*.{css,scss}",
    "lint": "run-p -s -c lint:*",
    "dev": "NODE_ENV='development' webpack serve --hot --config ./webpack/index.js",
    "build": "NODE_ENV='production' webpack --progress --config ./webpack/index.js",
    "analyze": "ANALYZE=1 nrun build",
    "size": "nrun build && size-limit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.13",
    "classnames": "2.2.6",
    "draft-js": "0.11.7",
    "first-follow": "^4.0.4",
    "lodash-es": "4.17.15",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hot-loader": "4.13.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@hot-loader/react-dom": "17.0.1",
    "@size-limit/preset-app": "4.9.1",
    "autoprefixer": "10.0.4",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.1",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "license-checker-webpack-plugin": "0.2.0",
    "lint-staged": "10.5.2",
    "mini-css-extract-plugin": "1.3.1",
    "npm-run-all": "4.1.5",
    "nrun": "1.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss-loader": "4.1.0",
    "sass": "1.29.0",
    "sass-loader": "10.1.0",
    "size-limit": "4.9.1",
    "style-loader": "2.0.0",
    "stylelint": "13.8.0",
    "stylelint-config-standard": "20.0.0",
    "terser-webpack-plugin": "4.2.3",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "4.2.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.4.0"
  },
  "browserslist": [
    "last 2 version",
    "not dead",
    "not ie <= 11"
  ],
  "size-limit": [
    {
      "limit": "1.5 s",
      "path": "build/*.js"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeDevice/first-follow.git"
  },
  "author": "Mikhail Vyrodov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MikeDevice/first-follow/issues"
  },
  "homepage": "https://github.com/MikeDevice/first-follow#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix"
  }
}
