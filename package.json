{
  "name": "first-follow",
  "version": "3.0.1",
  "description": "A small (762 bytes) tool for calculating first, follow and predict sets for a grammar",
  "main": "index.js",
  "scripts": {
    "docs": "documentation build lib/** -f md -o api.md",
    "test": "jest",
    "lint": "eslint ./",
    "size": "size-limit",
    "spellcheck": "nrun docs && yaspeller-ci *.md"
  },
  "size-limit": [
    {
      "limit": "762 B",
      "path": "index.js"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeDevice/first-follow.git"
  },
  "keywords": [
    "first",
    "follow",
    "predict",
    "set",
    "parser",
    "grammar"
  ],
  "author": "Mikhail Vyrodov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MikeDevice/first-follow/issues"
  },
  "homepage": "https://github.com/MikeDevice/first-follow#readme",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.2.4",
    "coveralls": "^3.0.9",
    "documentation": "^12.1.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "nrun": "^1.0.0",
    "yaspeller-ci": "^1.0.1"
  }
}
